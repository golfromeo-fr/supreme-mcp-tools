{
  "toolDirectories": [
    "tools/webmcp",
    "tools/oraclemcp",
    "tools/convertermcp",
    "tools/ragmcp",
    "tools/simplemcp8"
  ],
  "portAllocation": {
    "mode": "manual",
    "basePort": 8000,
    "portRange": [8000, 9000],
    "ports": {
      "oracleMCP": 8000,
      "web_mcp": 8001,
      "simplemcp8": 8002,
      "convertermcp": 8003,
      "ragmcp": 8004
    }
  },
  "server": {
    "host": "0.0.0.0",
    "logLevel": "info"
  },
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s",
    "file": null
  },
  "errorHandling": {
    "continueOnError": true,
    "failFast": false
  }
}
